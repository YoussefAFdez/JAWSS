{{ form_start(form) }}
    <div class="mb-6">
        {{ form_row(form.nombre) }}
    </div>
    <div class="mb-6">
        {{ form_row(form.descripcion) }}
    </div>
    <div class="flex items-start mb-6">
        <div class="flex items-center h-5 w-5">
            {{ form_widget(form.fichero) }}
        </div>
        {{ form_label(form.fichero) }}
    </div>
    <div class="mb-6">
        {{ form_row(form.extension) }}
    </div>
    <div class="mb-6">
        {{ form_row(form.propietario) }}
    </div>
    <div class="mb-6">
        {{ form_row(form.favorito) }}
    </div>
    <div class="mb-6">
        {{ form_row(form.usuarios) }}
    </div>
    <button class="text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 mr-2 mb-2 dark:bg-blue-600 dark:hover:bg-blue-700 focus:outline-none dark:focus:ring-blue-800">{{ button_label|default('Guardar') }}</button>
{{ form_end(form) }}
